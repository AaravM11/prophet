# 0G Integration
# Get your API key from https://0g.ai or your 0G provider
ZERO_G_API_KEY=your_0g_key_here

# Ethereum RPC URLs
# Sepolia testnet (for testnet deployments)
RPC_URL_SEPOLIA=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
